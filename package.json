    "mocha": "^2.3.4",
    "must": "^0.13.1",
    "ts-node": "^3.3.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": "^4.0.0"
  },
  "dependencies": {
    "@quenk/kindof": "^1.2.0",
    "@quenk/polate": "^1.3.0",
